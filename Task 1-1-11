class Vehicle:
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def display_info(self):
        print(f"Vehicle: {self.year} {self.make} {self.model}")

# Example usage:
car = Vehicle("Toyota", "Camry", 2022)
car.display_info()  # Output: Vehicle: 2022 Toyota Camry
